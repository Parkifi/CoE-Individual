function LoaderScreen(){}LoaderScreen.prototype.getLoader=function(){var e=Ti.UI.createView({zIndex:100,backgroundColor:"#77ffffff",width:"100%",height:"100%",top:0,visible:!1,left:0}),t=Ti.UI.createImageView({width:140,height:140,image:"/loadingScreen/circle.png"}),i=Ti.UI.createView({width:140,height:140,top:503,left:0,zIndex:10});i.add(t);var o=Ti.UI.createImageView({width:925,height:700,top:50,image:"/loadingScreen/loadbar.png"}),n=Ti.UI.createView({width:938}),r=Ti.UI.create2DMatrix();r=r.rotate("android"==Ti.Platform.osname?359:180);var a=Titanium.UI.createAnimation({transform:r,repeat:1e3,curve:Ti.UI.ANIMATION_CURVE_EASE_IN_OUT,duration:4e3});return"ipad"==Ti.Platform.osname&&(a.autoreverse=!0),t.animate(a),n.add(o),n.add(i),e.add(n),t.animate(a),e},module.exports=LoaderScreen;